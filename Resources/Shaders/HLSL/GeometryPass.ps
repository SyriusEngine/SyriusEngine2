struct VS_OUT{
    float4 position: SV_Position;
    float3 cameraPos: CameraPos;
    float2 texCoord: TexCoord;
    matrix<float, 3, 3> TBN: TBNMatrix;
};

struct PS_OUT{
    float4 fragColor: SV_TARGET0;
};

